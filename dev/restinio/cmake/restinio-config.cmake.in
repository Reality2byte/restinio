@PACKAGE_INIT@

set(RESTINIO_ASIO_SOURCE @RESTINIO_ASIO_SOURCE@)
set(RESTINIO_USE_SYSTEM_STANDALONE_ASIO @RESTINIO_USE_SYSTEM_STANDALONE_ASIO@)
set(RESTINIO_USE_SYSTEM_BOOST_ASIO @RESTINIO_USE_SYSTEM_BOOST_ASIO@)

set(RESTINIO_USE_SYSTEM_LLHTTP @RESTINIO_USE_SYSTEM_LLHTTP@)
set(RESTINIO_USE_SYSTEM_FMT @RESTINIO_USE_SYSTEM_FMT@)
set(RESTINIO_USE_SYSTEM_EXPECTED_LITE @RESTINIO_USE_SYSTEM_EXPECTED_LITE@)

include(CMakeFindDependencyMacro)

if(RESTINIO_ASIO_SOURCE STREQUAL "standalone")
    if(NOT RESTINIO_USE_SYSTEM_STANDALONE_ASIO)
        find_dependency(asio)
    endif ()
else ()
    if(NOT RESTINIO_USE_SYSTEM_BOOST_ASIO)
        find_dependency(boost)
    endif ()
endif ()

if (RESTINIO_USE_SYSTEM_LLHTTP)
    find_dependency(llhttp)
endif ()

if (NOT RESTINIO_USE_SYSTEM_FMT)
    find_dependency(fmt)
endif ()

if (NOT RESTINIO_USE_SYSTEM_EXPECTED_LITE)
    find_dependency(expected-lite)
endif ()

include(${CMAKE_CURRENT_LIST_DIR}/restinio-targets.cmake)
