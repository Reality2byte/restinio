@PACKAGE_INIT@

set(RESTINIO_ASIO_SOURCE @RESTINIO_ASIO_SOURCE@)
set(RESTINIO_DEPENDENCY_RESOLVE_HINT_STANDALONE_ASIO @RESTINIO_DEPENDENCY_RESOLVE_HINT_STANDALONE_ASIO@)
set(RESTINIO_DEPENDENCY_RESOLVE_HINT_BOOST_ASIO @RESTINIO_DEPENDENCY_RESOLVE_HINT_BOOST_ASIO@)

set(RESTINIO_DEPENDENCY_RESOLVE_HINT_LLHTTP @RESTINIO_DEPENDENCY_RESOLVE_HINT_LLHTTP@)
set(RESTINIO_DEPENDENCY_RESOLVE_HINT_FMT @RESTINIO_DEPENDENCY_RESOLVE_HINT_FMT@)
set(RESTINIO_DEPENDENCY_RESOLVE_HINT_EXPECTED_LITE @RESTINIO_DEPENDENCY_RESOLVE_HINT_EXPECTED_LITE@)

include(CMakeFindDependencyMacro)

if(RESTINIO_ASIO_SOURCE STREQUAL "standalone")
    if (RESTINIO_DEPENDENCY_RESOLVE_HINT_STANDALONE_ASIO STREQUAL "find_package")
        find_dependency(asio)
    endif ()
else ()
    if (RESTINIO_DEPENDENCY_RESOLVE_HINT_BOOST_ASIO STREQUAL "find_package")
        find_dependency(boost)
    endif ()
endif ()

if (RESTINIO_DEPENDENCY_RESOLVE_HINT_LLHTTP STREQUAL "find_package")
    find_dependency(llhttp)
endif ()

if (RESTINIO_DEPENDENCY_RESOLVE_HINT_FMT STREQUAL "find_package")
    find_dependency(fmt)
endif ()

if (RESTINIO_DEPENDENCY_RESOLVE_HINT_EXPECTED_LITE STREQUAL "find_package")
    find_dependency(expected-lite)
endif ()

include(${CMAKE_CURRENT_LIST_DIR}/restinio-targets.cmake)
